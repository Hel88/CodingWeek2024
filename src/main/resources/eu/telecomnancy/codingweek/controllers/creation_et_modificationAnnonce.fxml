<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="376.0" prefWidth="600.0"
xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" 
fx:controller="eu.telecomnancy.codingweek.controllers.CreationEtModificationAnnonceController"
stylesheets="@styles.css" styleClass="AnchorPane">
   <children>
      <Label fx:id="label" alignment="CENTER" layoutX="52.0" layoutY="14.0" prefHeight="46.0" prefWidth="497.0" text="Nouvelle annonce">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="addAnnonce" layoutX="445.0" layoutY="315.0" mnemonicParsing="false" onAction="#addAnnonce" prefHeight="27.0" prefWidth="120.0" text="Mettre en ligne" />
      <TextField fx:id="titre" layoutX="59.0" layoutY="89.0" promptText="titre" />
      <TextField fx:id="prix" layoutX="59.0" layoutY="120.0" promptText="prix" />
      <TextArea fx:id="description" layoutX="277.0" layoutY="80.0" prefHeight="200.0" prefWidth="262.0" promptText="description" wrapText="true" />
      <Label fx:id="messageErreur" layoutX="59.0" layoutY="164.0" prefHeight="72.0" prefWidth="170.0" text=" " wrapText="true" />
   </children>
</AnchorPane>
