<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0"
            xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="eu.telecomnancy.codingweek.controllers.MonProfilController"
            stylesheets="@styles.css" styleClass="AnchorPane">
    <children>
        <VBox layoutX="34.0" layoutY="136.0" prefHeight="200.0" prefWidth="239.0">
            <children>
                <Label fx:id="firstName" contentDisplay="CENTER" prefHeight="34.0" prefWidth="199.0" text="Prénom"
                       wrapText="true"/>
                <Label fx:id="lastName" prefHeight="34.0" prefWidth="199.0" text="Nom" wrapText="true"/>
                <Label fx:id="address" prefHeight="34.0" prefWidth="199.0" text="adresse" wrapText="true"/>
                <Label fx:id="email" prefHeight="34.0" prefWidth="199.0" text="email" wrapText="true"/>
                <Label fx:id="city" prefHeight="34.0" prefWidth="199.0" text="Ville" wrapText="true"/>
            </children>
        </VBox>
        <Label alignment="CENTER" layoutX="198.0" layoutY="27.0" prefHeight="34.0" prefWidth="205.0" text="Mon Profil"
               styleClass="LabelViolet">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label fx:id="username" alignment="CENTER" contentDisplay="CENTER" layoutX="187.0" layoutY="68.0"
               prefHeight="44.0" prefWidth="228.0" text="Nom d'utilisateur">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
        <Button layoutX="321.0" layoutY="236.0" mnemonicParsing="false" onAction="#displayPlanning" prefHeight="34.0"
                prefWidth="221.0" text="Voir/Modifier mon planning">
            <styleClass>
                <String fx:value="ButtonOrange"/>
                <String fx:value="ButtonArrondi"/>
            </styleClass>
        </Button>
        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="320.0" layoutY="135.0" prefHeight="34.0"
               prefWidth="221.0" text="Note donnée par les utilisateurs:"/>
        <Label fx:id="eval" alignment="CENTER" layoutX="405.0" layoutY="174.0" prefHeight="24.0" prefWidth="52.0"
               text="note"/>
        <Button fx:id="modifier" layoutX="439.0" layoutY="325.0" mnemonicParsing="false" onAction="#modifierProfil"
                prefHeight="34.0" prefWidth="135.0" text="Modifier Profil">
            <styleClass>
                <String fx:value="ButtonOrange"/>
                <String fx:value="ButtonArrondi"/>
            </styleClass>
        </Button>
        <Button fx:id="supprimer" layoutX="286.0" layoutY="325.0" mnemonicParsing="false" onAction="#supprimerProfil"
                prefHeight="34.0" prefWidth="135.0" text="Supprimer Profil">
            <styleClass>
                <String fx:value="ButtonOrange"/>
                <String fx:value="ButtonArrondi"/>
            </styleClass>
        </Button>
    </children>
</AnchorPane>
